<div class="container text-center">
  <div class="row row-cols-5">
    <div class="col">Name</div>
    <div class="col">Email</div>
    <div class="col">Contrasena</div>
    <div class="col">Date</div>
    <div class="col"></div>
  </div>
{{#each usuario}}
<hr>
  <div class="container">
    <div class="row row-cols-5">
      <div class="col">{{this.usuario}}</div>
      <div class="col">{{this.email}}</div>
      <div class="col">{{this.contrasena}}</div>
      <div class="col">{{this.fecha}}</div>
      <div class="col"><a href="/user-delete/{{this.id}}" class="btn btn-danger">Eliminar</a></div>
    </div>
  </div>
{{/each}}
  <a href="/user-create/{{this.id}}" class="text-center btn btn-primary">Agregar Usuario</a>
</div>